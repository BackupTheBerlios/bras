Newer {bras.dvi bras.aux} {bras.ind bras.tex} {
  latex bras
  latex bras
}

Newer bras.ps bras.dvi {
  dvips -q bras
}

Newer bras.pdf bras.tex {
  pdflatex bras
  pdflatex bras
}


Always bras.ind {} {
  latex bras
  latex bras
  makeindex -q bras
}
