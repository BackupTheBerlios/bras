puts "msg: several rules for a target"

puts "expect: rm -f a.o"
puts "expect: touch a.c bla.h bli.h blu.h voodo.h turilu.h"
puts "expect: deps = `a.c bla.h bli.h blu.h voodo.h turilu.h'"
puts "expect: trigger = `a.c bla.h bli.h blu.h voodo.h turilu.h'"
puts "expect: rm -f a.c bla.h bli.h blu.h voodo.h turilu.h"

exec rm -f a.o

Newer a.o {a.c bla.h} {}
Newer a.o {bli.h blu.h}
Newer a.o {voodo.h turilu.h}
Newer a.o {} {
  puts "deps = `$deps'"
  puts "trigger = `$trigger'"
  rm -f $trigger
}

## This is a catch all rule which creates all dependencies
## of a.o
Exist [GetDeps a.o] {
  touch $targets
}
