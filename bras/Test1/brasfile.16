puts "msg: Several targets in one rule (simple case)"

puts "expect: rm -f cmdline.c cmdline.h cmdline.cli"
puts "expect: touch cmdline.cli"
puts "expect: touch cmdline.c cmdline.h"
puts "expect: all <- cmdline.h cmdline.c"
puts "expect: rm -f cmdline.c cmdline.h cmdline.cli"

exec rm -f cmdline.c cmdline.h cmdline.cli

Newer all {cmdline.h cmdline.c} {
  puts "all <- $trigger"
  rm -f cmdline.c cmdline.h cmdline.cli
}

Newer {cmdline.h cmdline.c} {cmdline.cli} {
  touch cmdline.c cmdline.h
}

Exist cmdline.cli {} {
  touch cmdline.cli
}
